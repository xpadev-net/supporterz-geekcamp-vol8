{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint&&npm run eslint&&npm run check-types",
    "check-types": "npx tsc --noEmit",
    "eslint": "eslint */**/*.ts*",
    "eslint:fix": "eslint */**/*.ts* --fix",
    "lint:fix": "npm run format&&npm run eslint:fix&&npm run check-types",
    "format": "prettier --write \"**/*.{tsx,ts,js,json,css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "next": "13.0.5",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.56.1",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "scss": "^0.2.4"
  }
}
