{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "rootDir": "./",
    "paths": {
      "@/components/*": [
        "./components/*"
      ],
      "@/styles/*": [
        "./styles/*"
      ],
      "@/public/*": [
        "./public/*"
      ],
      "@/pages/*": [
        "./pages/*"
      ],
      "@/libs/*": [
        "./libs/*"
      ],
      "@/contexts/*": [
        "./contexts/*"
      ]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
